{
  "schemaVersion": 1,
  "id": "ghuperms",
  "version": "${version}",
  "name": "GhuPerms",
  "description": "Permission manager mod",
  "authors": [
    "Skyfy16"
  ],
  "contact": {},
  "license": "MIT",
  "icon": "assets/ghuperms/icon.png",
  "environment": "server",
  "entrypoints": {
    "server": [
      {
        "adapter": "kotlin",
        "value": "ch.skyfy.ghuperms.GhuPermsMod"
      }
    ],
    "preLaunch": [
      {
        "adapter": "kotlin",
        "value": "ch.skyfy.ghuperms.prelaunch.GhuPermsPreLauncher"
      }
    ],
    "main": []
  },
  "mixins": [
    "ghuperms.mixin.json",
    "prelaunch.ghuperms.mixin.json"
  ],
  "depends": {
    "java": ">=17",
    "minecraft": "1.20.1",
    "fabric": "*",
    "fabricloader": ">=0.14.22",
    "fabric-language-kotlin": ">=1.9.6+kotlin.1.8.22"
  }
}
